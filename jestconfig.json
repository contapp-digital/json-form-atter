{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/*.(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "js", "tsx", "node"],
  "collectCoverage": true,
  "collectCoverageFrom": ["./src/types/**", "./src/utils/**"],
  "coverageThreshold": {
    "global": {
      "lines": 90
    }
  }
}